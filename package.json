    "author": "Jameson Heesen",
    "license": "MIT",
    "devDependencies": {
        "electron": "^3.0.5",
        "electron-packager": "^12.0.0",
        "electron-rebuild": "^1.8.2",
        "flow-bin": "^0.83.0"
    },
    "dependencies": {
        "electron-spellchecker": "^1.1.2",
        "node-abi": "^2.4.4"
    }
}
